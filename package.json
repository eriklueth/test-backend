{
  "name": "graphql-api",
  "version": "1.0.0",
  "description": "GraphQL Backend for Dashpoint App",
  "main": "index.ts",
  "author": "Dashpoint GmbH",
  "license": "UNLICENSED",
  "scripts": {
    "build": "tsc",
    "types:windows": "set NODE_ENV=development&& set GENERATE='true' && tsc --noEmit && node ./dist/index.js",
    "types:unix": "export NODE_ENV=development&& set GENERATE='true' && tsc --noEmit && node ./dist/index.js",
    "deploy": "tsc && gcloud app deploy",
    "dev:windows": "set NODE_ENV=development&& tsc && node ./dist/index.js",
    "dev:unix": "export NODE_ENV=development&& tsc && node ./dist/index.js",
    "dev": "echo Please run either npm run dev:windows or npm run dev:unix",
    "prod": "set NODE_ENV=production&& export NODE_ENV=production&&tsc && node ./dist/index.js"
  },
  "dependencies": {
    "@apollo/server": "^4.7.0",
    "@apollo/server-plugin-response-cache": "^4.1.3",
    "@neo4j/graphql": "^4.3.2",
    "@neo4j/graphql-amqp-subscriptions-engine": "^1.0.0",
    "@neo4j/graphql-ogm": "^4.3.2",
    "cors": "^2.8.5",
    "debug": "4.3.4",
    "dotenv": "^16.0.2",
    "express": "^4.18.2",
    "firebase": "^10.5.0",
    "firebase-admin": "^11.11.0",
    "global": "^4.4.0",
    "graphql": "^16.7.1",
    "graphql-tag": "^2.12.6",
    "graphql-upload": "^13.0.0",
    "graphql-ws": "^5.11.2",
    "handlebars": "^4.7.8",
    "html-pdf": "^3.0.1",
    "html-pdf-node": "^1.0.8",
    "jsonwebtoken": "^9.0.0",
    "neo4j-driver": "^5.8.0",
    "node-fetch": "^2.6.7",
    "request": "^2.88.2",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@types/bcrypt": "5.0.0",
    "@types/cors": "^2.8.13",
    "@types/debug": "4.1.7",
    "@types/dotenv": "8.2.0",
    "@types/express": "4.17.9",
    "@types/graphql-upload": "^8.0.11",
    "@types/jest": "28.1.4",
    "@types/jsonwebtoken": "8.5.1",
    "@types/node": "16.11.41",
    "@types/node-fetch": "^2.6.2",
    "@types/semver": "^7.3.12",
    "@types/uuid": "8.3.4",
    "@types/ws": "^8.5.5",
    "jest": "28.1.2",
    "nodemon": "2.0.19",
    "randomstring": "1.1.5",
    "ts-jest": "28.0.5",
    "ts-node": "10.8.2",
    "tsconfig-paths": "4.0.0",
    "typescript": "4.7.4"
  }
}
